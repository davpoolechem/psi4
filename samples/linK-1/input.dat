#! RHF Linear Exchange Algorithm test for water


molecule mol {
    0 1
    O
    H 1 0.96
    H 1 0.96 2 104.5
    symmetry c2v
    no_reorient
    no_com
}

set {
    scf_type dfdirj+link 
    guess sad 
    df_scf_guess false
    basis 6-31g(d) 
    e_convergence 1.0e-10
    screening density 
    incfock true 
    incfock_full_fock_every 4
}

linK_energy = energy('scf')
